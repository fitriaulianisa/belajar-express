<h1>Prodi</h1>
<table class="table table-striped">
    <tr>
        <th>Nama Prodi</th>
        <th>Fakultas</th>
        <th>Singkatan</th>
    </tr>
    <% prodi.forEach(data => { %>
    <tr>
        <td><%= data.namaProdi %></td>
        <td><%= data.singkatan %></td>
    </tr>
        
    <% }) %>

</table>